.welcome{
    display: flex;
    flex-direction: column;
    align-items: center;
   
}
.welcome image{
    width: 200rpx;
    height: 200rpx;
    border-radius: 50%;
    margin-top: 100px; 
}
.welcome text{
    font-size: 30rpx;
    font-weight: bold;
    margin-top: 30px;
}
.welcome button{
    width: 300rpx;
    height: 100rpx;
    margin-top: 30rpx;
    font-size: 20rpx;
    line-height: 100rpx;
    background-color: #b3d4db;
    border: 1px solid #435e63;
}
page{
